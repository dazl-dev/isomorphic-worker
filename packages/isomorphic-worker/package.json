{
  "name": "@wixc3/isomorphic-worker",
  "version": "1.2.5",
  "type": "module",
  "description": "simplified cross worker API for web workers and worker threads",
  "homepage": "https://github.com/wixplosives/isomorphic-worker",
  "exports": {
    "./worker-scope": {
      "browser": "./dist/browser-worker-scope.js",
      "node": "./dist/node-worker-scope.js",
      "types": "./dist/node-worker-scope.d.ts",
      "default": "./dist/node-worker-scope.js"
    },
    "./worker": {
      "browser": "./dist/browser-worker.js",
      "node": "./dist/node-worker.js",
      "types": "./dist/node-worker.d.ts",
      "default": "./dist/node-worker.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "default": "./dist/types.js"
    },
    "./package": "./package.json",
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "src",
    "!*/tsconfig.{json,tsbuildinfo}"
  ],
  "keywords": [
    "worker threads",
    "worker_threads",
    "webworker",
    "web worker",
    "web-worker",
    "worker",
    "threads"
  ],
  "bugs": {
    "url": "https://github.com/wixplosives/isomorphic-worker/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wixplosives/isomorphic-worker.git",
    "directory": "packages/isomorphic-worker"
  },
  "author": "Wix.com",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false
}
